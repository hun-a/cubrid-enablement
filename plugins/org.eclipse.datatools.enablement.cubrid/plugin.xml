<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
	
	
	<extension
		point="org.eclipse.datatools.connectivity.sqm.core.catalog">
		<catalog
			version="9.3.6.0002"
			product="CUBRID"
	      	provider="org.eclipse.datatools.enablement.cubrid.catalog.CUBRIDCatalogProvider">
		</catalog>
	</extension>
	<extension
		point="org.eclipse.datatools.connectivity.sqm.core.ddlGeneration">
		<generator
			version="9.3.6.0002"
			product="CUBRID"
			class="org.eclipse.datatools.enablement.cubrid.ddl.CUBRIDDdlGenerator">
		</generator>
	</extension>
	
	
	<extension
		point="org.eclipse.datatools.connectivity.connectionProfile">
		<connectionProfile
			category="org.eclipse.datatools.connectivity.db.category"
			configurationType="org.eclipse.datatools.connectivity.db.generic.configurationType"
			icon="icons/jdbc_16.gif"
			id="org.eclipse.datatools.enablement.cubrid.connectionProfile"
			name="%profile.cubrid.title"
			pingFactory="org.eclipse.datatools.enablement.internal.cubrid.connection.JDBCCUBRIDConnectionFactory"
			propertiesPersistenceHook="org.eclipse.datatools.enablement.internal.cubrid.connection.CUBRIDPasswordPropertiesPersistenceHook"/>
		<connectionFactory
			class="org.eclipse.datatools.enablement.internal.cubrid.connection.JDBCCUBRIDConnectionFactory"
			id="java.sql.Connection"
			name="%jdbc.connection.name"
			profile="org.eclipse.datatools.enablement.cubrid.connectionProfile"/>
	</extension>
	
	<extension
		point="org.eclipse.datatools.connectivity.driverExtension">
		<category
			parentCategory="org.eclipse.datatools.connectivity.db.driverCategory"
			id="org.eclipse.datatools.enablement.cubrid.driverCategory"
			name="%CUBRID_CATEGORY_NAME"/>
		<category
			parentCategory="org.eclipse.datatools.enablement.cubrid.driverCategory"
			id="org.eclipse.datatools.enablement.cubrid.9_3_6.driverCategory"
			name="%9_3_6_VERSION_CATEGORY_NAME"/>
			
		

		<driverTemplate
			createDefault="false"
			emptyJarListIsOK="false"
			id="org.eclipse.datatools.enablement.cubrid.9_3_6.driverTemplate"
			jarList="JDBC-9.3.6.0002-cubrid.jar"
			name="%org.eclipse.datatools.enablement.cubrid.9_3_6_VERSION_CATEGORY_NAME"
			parentCategory="org.eclipse.datatools.enablement.cubrid.9_3_6.driverCategory">
			
			<properties>
				<property
					generated="false"
					id="org.eclipse.datatools.connectivity.db.driverClass"
					name="%driverClass"
					value="cubrid.jdbc.driver.CUBRIDDriver"
					required="true"
					visible="true"/>
				<property
					generated="false"
					id="org.eclipse.datatools.connectivity.db.vendor"
					name="%driverVender"
					value="CUBRID"
					required="true"
					visible="false"/>
				<property
					generated="false"
					id="org.eclipse.datatools.connectivity.db.version"
					name="%driverVersion"
					value="9.3.6.0002"
					required="true"
					visible="false"/>
				<property
					generated="false"
					id="org.eclipse.datatools.connectivity.db.databaseName"
					name="%driverDBName"
					value="database"
					required="true"
					visible="true"/>
				<property
					generated="false"
					id="org.eclipse.datatools.connectivity.db.URL"
					name="%driverConnectionURL"
					value="jdbc:cubrid:localhost:33000:demodb:::"
					required="true"
					visible="true"/>
				<property
					generated="false"
					id="org.eclipse.datatools.connectivity.db.username"
					name="%driverUID"
					value="dba"
					required="false"
					visible="true"/>
				<property
					generated="false"
					id="org.eclipse.datatools.connectivity.db.connectionProperties"
					name="%driverCP"
					value="zeroDateTimeBehavior=convertToNull"
					required="false"
					visible="false"/>
				<property
					generated="false"
					id="org.eclipse.datatools.connectivity.db.password"
					name="%driverPWD"
					required="false"
					visible="true"/>
			</properties>
		</driverTemplate>

	</extension>
	
	<extension
		point="org.eclipse.datatools.connectivity.sqm.core.catalog">
		<overrideLoader
			eclass="org.eclipse.datatools.modelbase.sql.constraints.TableConstraint"
			product="CUBRID"
			provider="org.eclipse.datatools.enablement.cubrid.catalog.loaders.CUBRIDTableConstraintLoader"
			version="9.3.6.0002">
		</overrideLoader>
		<overrideLoader
			eclass="org.eclipse.datatools.modelbase.sql.tables.Table"
			product="CUBRID"
			provider="org.eclipse.datatools.enablement.cubrid.catalog.loaders.CUBRIDTableLoader"
			version="9.3.6.0002">
		</overrideLoader>
		<overrideLoader
			eclass="org.eclipse.datatools.modelbase.sql.tables.Column"
			product="CUBRID"
			provider="org.eclipse.datatools.enablement.cubrid.catalog.loaders.CUBRIDTableColumnLoader"
			version="9.3.6.0002">
		</overrideLoader>
		<overrideLoader
			eclass="org.eclipse.datatools.modelbase.sql.constraints.Index"
			product="CUBRID"
			provider="org.eclipse.datatools.enablement.cubrid.catalog.loaders.CUBRIDTableIndexLoader"
			version="9.3.6.0002">
		</overrideLoader>
		<overrideLoader
			eclass="org.eclipse.datatools.modelbase.sql.routines.Routine"
			product="CUBRID"
			provider="org.eclipse.datatools.enablement.cubrid.catalog.loaders.CUBRIDRoutineLoader"
			version="9.3.6.0002">
		</overrideLoader>
  <overrideLoader
        eclass="org.eclipse.datatools.modelbase.sql.accesscontrol.AuthorizationIdentifier"
        product="CUBRID"
        provider="org.eclipse.datatools.enablement.cubrid.catalog.loaders.CUBRIDAuthorizationIdentifierLoader"
        version="9.3.6.0002">
  </overrideLoader>
	</extension>
	
	<extension
		point="org.eclipse.datatools.connectivity.sqm.core.mappings">
		<providerIDtoDriverCategoryID
			driverCategoryID="org.eclipse.datatools.enablement.cubrid.driverCategory"
			providerID="org.eclipse.datatools.enablement.cubrid.connectionProfile">
		</providerIDtoDriverCategoryID>
	</extension>
</plugin>
